<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>cron</web-resource-name>
			<url-pattern>/cron/*</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>secret</web-resource-name>
			<url-pattern>/assets/secret/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>
	<servlet>
		<description></description>
		<display-name>GitKitWidgetServlet</display-name>
		<servlet-name>GitKitWidgetServlet</servlet-name>
		<servlet-class>com.appspot.movevote.controller.GitKitWidgetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GitKitWidgetServlet</servlet-name>
		<url-pattern>/gitkit</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>InSingCronServlet</display-name>
		<servlet-name>InSingCronServlet</servlet-name>
		<servlet-class>com.appspot.movevote.cron.InSingCronServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InSingCronServlet</servlet-name>
		<url-pattern>/cron/insing</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>HomeServlet</display-name>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.appspot.movevote.controller.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>MovieServlet</display-name>
		<servlet-name>MovieServlet</servlet-name>
		<servlet-class>com.appspot.movevote.controller.MovieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MovieServlet</servlet-name>
		<url-pattern>/movie</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>SignOutServlet</display-name>
		<servlet-name>SignOutServlet</servlet-name>
		<servlet-class>com.appspot.movevote.controller.SignOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignOutServlet</servlet-name>
		<url-pattern>/sign_out</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserServlet</display-name>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.appspot.movevote.controller.UserProfileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/user_profile</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>home</welcome-file>
	</welcome-file-list>
</web-app>
